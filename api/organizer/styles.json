{
  headings: {
    syntax: {
      html: '<h>',
      markdown: '#',
      orgmode: '*'
    }
  },
  lists: {
    ordered: {
      syntax: {
        html: '<li>',
        markdown: '/[0-9]+\./',
        orgmode: '/[0-9]+\./'
      },
      wrapper: {
        syntax: {
          html: '<ol>'
        }
      }
    },
    unordered: {
      syntax: {
        html: '<li>',
        markdown: '+',
        orgmode: '+'
      },
      wrapper: {
        syntax: {
          html: '<ul>'
        }
      }
    },
    emphasis: {
      bold: {
        syntax: {
          html: '<b>',
          markdown: '**',
          orgmode: '*'
        }
      },
      italic: {
        syntax: {
          html: '<i>',
          markdown: '*',
          orgmode: '/'
        }
      },
      strikethrough: {
        syntax: {
          html: '<strike>',
          markdown: '~~',
          orgmode: '+'
        }
      },
      underline: {
      }
    },
    links: {
      text: {
        syntax: {
          html: '',
          markdown: '[]',
          orgmode: '[]'
        }
      },
      url: {
        syntax: {
          html: 'href=',
          markdown: '()',
          orgmode: '[]'
        }
      },
      wrapper: {
        syntax: {
          html: '<a>',
          markdown: '',
          orgmode: '[]'
        }
      }
    }
  }
}
